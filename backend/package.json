{
  "name": "alcahub-backend",
  "version": "1.0.0",
  "description": "Backend do Alça Hub - API para prestadores de serviço",
  "main": "server.py",
  "scripts": {
    "import:providers": "ts-node scripts/import_providers.ts",
    "test:import": "ts-node scripts/test_import.ts",
